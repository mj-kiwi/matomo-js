{
  "name": "@mj-kiwi/matomo-client",
  "version": "0.1.0",
  "type": "module",
  "license": "MIT",
  "author": {
    "name": "MJ Kiwi",
    "email": "me@mj.kiwi"
  },
  "repository": {
    "url": "https://github.com/mj-kiwi/matomo-js"
  },
  "keywords": [
    "matomo",
    "analytics",
    "reporting",
    "api",
    "web-analytics",
    "typescript",
    "client",
    "metrics",
    "tracking",
    "statistics",
    "piwik"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:unit": "vitest run spec",
    "test:integration": "vitest run integration",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "@vitest/coverage-v8": "^3.1.1",
    "nock": "^14.0.3",
    "tsup": "^8.4.0",
    "typescript": "^5.2.2",
    "vitest": "^3.1.1"
  },
  "dependencies": {
    "axios": "^1.6.7"
  }
}
